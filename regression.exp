set failureMsg "FAILURE"
set timeoutMsg "TIMEOUT"
set timeout 1
set ctl_c       \003\n
set userName     [exec whoami]
set outputFile Regression_Test.txt

spawn -noecho bash

## Logging and debugging
log_user 0
log_file -noappend $outputFile
## Debug purposes
exp_internal 0


